CURSOR = b''
X_hot, Y_hot = 4, 18

PRESENTER_FRAME   = ((100., 100.), (1024., 768.))
FEED_HEIGHT = 40
MINIATURE_WIDTH, MINIATURE_MARGIN = 120, 5
MIN_POSTER_HEIGHT = 20.

CR, ESC, DEL = (chr(k) for k in [13, 27, 127])

